  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.18.0",
    "react-hook-form": "^7.48.0",
    "zod": "^3.22.2",
    "axios": "^1.5.0",
    "zustand": "^4.4.1"
